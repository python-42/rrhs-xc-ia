<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1250.0" stylesheets="@all.css" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rrhs.xc.ia.ui.controller.ScheduleController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TableView fx:id="table" editable="true" fixedCellSize="40.0" layoutY="50.0" prefHeight="700.0" prefWidth="1250.0">
               <placeholder>
                  <Label text="No meets in the database." />
               </placeholder>
               <columns>
                  <TableColumn fx:id="nameCol" editable="true" prefWidth="524.0" text="Name" />
                  <TableColumn fx:id="dateCol" editable="true" prefWidth="368.0" text="Date" />
                  <TableColumn fx:id="changedCol" editable="false" prefWidth="357.0" text="Unsaved Changes" />
               </columns>
            </TableView>
            <Label layoutX="25.0" layoutY="5.0" text="Edit Schedule">
               <font>
                  <Font name="IBM Plex Mono Light" size="40.0" />
               </font>
            </Label>
            <Label layoutX="1086.0" layoutY="13.0" text="Go Huskies!" textFill="#094be8">
               <font>
                  <Font name="IBM Plex Mono Light" size="25.0" />
               </font>
            </Label>
            <Button fx:id="cancelBtn" layoutX="670.0" layoutY="755.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="260.0" styleClass="cancel" text="Cancel">
               <font>
                  <Font name="IBM Plex Mono Light" size="20.0" />
               </font>
            </Button>
            <Button fx:id="saveBtn" layoutX="950.0" layoutY="755.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="260.0" styleClass="save" text="Save">
               <font>
                  <Font name="IBM Plex Mono Light" size="20.0" />
               </font>
            </Button>
            <Button fx:id="addBtn" layoutX="50.0" layoutY="755.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="260.0" text="Add Meet">
               <font>
                  <Font name="IBM Plex Mono Light" size="20.0" />
               </font>
            </Button>
            <Button fx:id="deleteBtn" layoutX="330.0" layoutY="755.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="260.0" text="Delete Selected">
               <font>
                  <Font name="IBM Plex Mono Light" size="20.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
